{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#c6a0f6",
    "sapphire": "#7dc4e4",
    "closer": "p:os",
    "text": "#cad3f5",
    "mauve": "#c6a0f6",
    "lavender": "#B7BDF8",
    "pink": "#f5bde6",
    "blue": "#8AADF4"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:text",
          "style": "plain",
          "template": " {{.Icon}} {{ .UserName }} ",
          "type": "os"
        },
        {
          "foreground": "p:mauve",
          "properties": {
            "folder_icon": ".",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "p:text",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "p:mauve",
          "style": "plain",
          "template": "x ",
          "type": "status"
        },
        {
          "foreground": "p:text",
          "properties": {},
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7a7a7a> ",
          "style": "plain",
          "type": "python"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
