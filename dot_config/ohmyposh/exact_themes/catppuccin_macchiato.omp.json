{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#8AADF4",
    "closer": "p:os",
    "text": "#cad3f5",
    "lavender": "#B7BDF8",
    "os": "#c6a0f6",
    "mauve": "#c6a0f6"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\uf303 ",
          "foreground": "p:blue",
          "type": "os",
          "style": "plain"
        },
        {
          "template": "{{ .UserName }} ",
          "foreground": "p:blue",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "template": "{{ .Path }} ",
          "foreground": "p:mauve",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "foreground": "p:lavender",
          "type": "git",
          "style": "plain"
        },
        {
          "foreground": "p:text",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "p:mauve",
          "style": "plain",
          "template": "x ",
          "type": "status"
        },
        {
          "foreground": "p:text",
          "properties": {},
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#7a7a7a> ",
          "style": "plain",
          "type": "python"
        },
        {
          "template": "\uf105",
          "foreground": "p:closer",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
